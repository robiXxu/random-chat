{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console":false,
    "member-access": false,
    "ordered-imports": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "label-position": false
  }
}
